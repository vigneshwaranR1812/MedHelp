<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>ABC</servlet-name>
    <servlet-class>Database</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>SignUp</servlet-name>
    <servlet-class>SignUp</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Login</servlet-name>
    <servlet-class>Login</servlet-class>
  </servlet>
  <servlet>
     <servlet-name>AddMed</servlet-name>
     <servlet-class>AddMed</servlet-class>
  </servlet>

    <servlet-mapping>
    <servlet-name>ABC</servlet-name>
    <url-pattern>/abc</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SignUp</servlet-name>
    <url-pattern>/Login_v3/signUp</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/Login_v3/login</url-pattern>
  </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddMed</servlet-name>
        <url-pattern>/addMed</url-pattern>
    </servlet-mapping>
</web-app>
